{
  "name": "codefiction-answerminer-watermark",
  "author": "denolk",
  "description": "codefiction watermark for answerminer graphs",
  "version": "1.0.0",
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "48": "icon-32.png",
    "64": "icon-64.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "http://app.answerminer.com/*",
    "https://app.answerminer.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://app.answerminer.com/*",
        "http://app.answerminer.com/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "manifest_version": 2
}