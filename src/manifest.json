{
  "name": "codefiction-answerminer-watermark",
  "author": "denolk",
  "description": "codefiction watermark for answerminer graphs",
  "version": "1.0.0",
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "48": "icon-32.png",
    "64": "icon-64.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "storage", "declarativeContent"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon-16.png",
      "32": "icon-32.png",
      "48": "icon-32.png",
      "64": "icon-64.png",
      "128": "icon-128.png"
    }
  },
  "manifest_version": 2
}